{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 21,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "Release",
      "displayName": "Release",
      "description": "Default Release build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      },
      "binaryDir": "cmake-build-Release"
    },
    {
      "name": "Debug",
      "displayName": "debug",
      "description": "Debug build without sanitizers",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      },
      "binaryDir": "cmake-build-Debug"
    }
  ]
}
